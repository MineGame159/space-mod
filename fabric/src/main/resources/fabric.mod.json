{
    "schemaVersion": 1,
    "id": "${id}",
    "version": "${version}",
    "name": "Space Mod",
    "description": "Yes.",
    "license": "MIT",
    "environment": "*",
    "entrypoints": {
        "main": [
            "minegame159.spacemod.fabric.SpaceModFabric"
        ],
        "client": [
            "minegame159.spacemod.fabric.client.SpaceModFabricClient"
        ],
        "server": [
            "minegame159.spacemod.fabric.server.SpaceModFabricServer"
        ],
        "jade": [
            "minegame159.spacemod.compat.jade.SpaceModJadePlugin"
        ],
        "emi": [
            "minegame159.spacemod.compat.emi.SpaceModEmiPlugin"
        ]
    },
    "mixins": [
        "spacemod.mixins.json",
        "spacemod-fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "minecraft": "~${minecraft_version}",
        "java": ">=21",
        "architectury": ">=${architectury_api_version}",
        "fabric-api": "*",
        "team_reborn_energy": ">=${techreborn_energy_version}"
    },
    "suggests": {
        "jade": ">=${jade_version}",
        "emi": ">=${emi_version}"
    }
}
